<BorderPane layoutY="3.0" prefHeight="225.0" prefWidth="275.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.onebeartoe.games.press.your.button.fx.FXMLController">
   <bottom>
      <HBox alignment="CENTER" fillHeight="false" BorderPane.alignment="CENTER">
         <children>
                  <Button fx:id="newGameButton" mnemonicParsing="false" onAction="#handleNewGameButton" text="New Game" />
            <Button fx:id="nextPlayerButton" mnemonicParsing="false" text="Next Player" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </HBox>
   </bottom>
   <center>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">

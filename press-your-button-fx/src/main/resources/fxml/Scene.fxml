                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label fx:id="board00" text="Label" />
                        <Label fx:id="board01" text="Label" GridPane.columnIndex="1" />
                        <Label fx:id="board02" text="Label" GridPane.columnIndex="2" />
                        <Label fx:id="board03" text="Label" GridPane.columnIndex="3" />
                        <Label fx:id="board04" text="Label" GridPane.columnIndex="4" />
                     </children>
                  </GridPane>
               </top>

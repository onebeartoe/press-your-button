                    <Button fx:id="button" onAction="#handleButtonAction" text="Stop" BorderPane.alignment="CENTER" />
         </right>
         <left>
            <VBox alignment="CENTER" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Label" />
                  <Label text="Label" />
                  <Label text="Label" />
               </children>
            </VBox>
         </left>
         <center>
            <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <center>
                  <Label fx:id="playerLabel" prefHeight="49.0" prefWidth="111.0" text="Player 1" textAlignment="CENTER" BorderPane.alignment="CENTER">
                     <font>
                        <Font name="Arial" size="29.0" />
                     </font>
                  </Label>
               </center>
               <bottom>
                  <GridPane BorderPane.alignment="CENTER">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
